<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="https://jakarta.ee/xml/ns/jakartaee"
              xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_4_0.xsd"
              version="4.0">
    
    <name>xpert</name>
    <ordering>
        <before>
            <name>primefaces</name>
        </before>
    </ordering>
    <application>
        <resource-bundle>
            <base-name>com.xpert.messages</base-name>
            <var>xmsg</var>
        </resource-bundle>
        <system-event-listener>
            <source-class>jakarta.faces.application.Application</source-class>
            <system-event-class>jakarta.faces.event.PostConstructApplicationEvent</system-event-class>
            <system-event-listener-class>com.xpert.PostConstructApplicationEventListener</system-event-listener-class>
        </system-event-listener>
    </application>
    
    
    <converter>
        <converter-id>entityConverterList</converter-id>
        <converter-class>com.xpert.faces.conversion.EntityConverterList</converter-class>
    </converter>
    <converter>
        <converter-id>entityConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.EntityConverter</converter-class>
    </converter>
    <converter>
        <converter-id>activeInactiveConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.ActiveInactiveConverter</converter-class>
    </converter>
    <converter>
        <converter-id>cnpjConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.CnpjConverter</converter-class>
    </converter>
    <converter>
        <converter-id>cpfCnpjConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.CpfCnpjConverter</converter-class>
    </converter>
    <converter>
        <converter-id>cpfConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.CpfConverter</converter-class>
    </converter>
    <converter>
        <converter-id>dataSerproConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.DataSerproConverter</converter-class>
    </converter>
    <converter>
        <converter-id>dataHoraSerproConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.DataHoraSerproConverter</converter-class>
    </converter>
    <converter>
        <converter-id>descricaoEnumSerproConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.DescricaoEnumSerproConverter</converter-class>
    </converter>
    <converter>
        <converter-id>placaCarroConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.PlacaCarroConverter</converter-class>
    </converter>
    <converter>
        <converter-id>yesNoConverter</converter-id>
        <converter-class>com.xpert.faces.conversion.YesNoConverter</converter-class>
    </converter>
    
    <validator>
        <validator-id>cnpjValidator</validator-id>
        <validator-class>com.xpert.faces.validation.CnpjValidator</validator-class>
    </validator>
    <validator>
        <validator-id>cpfValidator</validator-id>
        <validator-class>com.xpert.faces.validation.CpfValidator</validator-class>
    </validator>
    <validator>
        <validator-id>latitudeValidator</validator-id>
        <validator-class>com.xpert.faces.validation.LatitudeValidator</validator-class>
    </validator>
    <validator>
        <validator-id>longitudeValidator</validator-id>
        <validator-class>com.xpert.faces.validation.LongitudeValidator</validator-class>
    </validator>
    <validator>
        <validator-id>renavamValidator</validator-id>
        <validator-class>com.xpert.faces.validation.RenavamValidator</validator-class>
    </validator>
    
    <behavior>
        <behavior-id>xpert.faces.DownloadBehavior</behavior-id>
        <behavior-class>com.xpert.faces.behavior.DownloadBehavior</behavior-class>
    </behavior>
    <behavior>
        <behavior-id>xpert.faces.PDFPrinterBehavior</behavior-id>
        <behavior-class>com.xpert.faces.component.pdfprinter.PDFPrinterBehavior</behavior-class>
    </behavior>
    <component>
        <component-type>com.xpert.faces.component.Spread</component-type>
        <component-class>com.xpert.faces.component.spread.Spread</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.SecurityArea</component-type>
        <component-class>com.xpert.faces.component.security.SecurityArea</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.DateFilter</component-type>
        <component-class>com.xpert.faces.component.datefilter.DateFilter</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.Mask</component-type>
        <component-class>com.xpert.faces.component.mask.Mask</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.InputNumber</component-type>
        <component-class>com.xpert.faces.component.inputnumber.InputNumber</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.Cpf</component-type>
        <component-class>com.xpert.faces.component.cpf.Cpf</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.Cnpj</component-type>
        <component-class>com.xpert.faces.component.cnpj.Cnpj</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.RestorableFilter</component-type>
        <component-class>com.xpert.faces.component.restorablefilter.RestorableFilter</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.Group</component-type>
        <component-class>com.xpert.faces.component.group.Group</component-class>
    </component>
    <component>
        <component-type>com.xpert.faces.component.Initializer</component-type>
        <component-class>com.xpert.faces.component.initializer.Initializer</component-class>
    </component>
    <render-kit>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.SpreadRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.spread.SpreadRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.SecurityAreaRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.security.SecurityAreaRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.DateFilterRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.datefilter.DateFilterRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.MaskRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.mask.MaskRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.RestorableFilterRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.restorablefilter.RestorableFilterRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.GroupRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.group.GroupRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.InputNumberRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.inputnumber.InputNumberRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.CpfRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.cpf.CpfRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>com.xpert.component</component-family>
            <renderer-type>com.xpert.faces.component.CnpjRenderer</renderer-type>
            <renderer-class>com.xpert.faces.component.cnpj.CnpjRenderer</renderer-class>
        </renderer>
    </render-kit>
</faces-config>
